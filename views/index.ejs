        @import url('https://fonts.googleapis.com/css2?family=EB+Garamond:wght@400;700&family=Montserrat:wght@400;700&display=swap');

.dynamic-section {
    padding: 50px 5%;
    background-color: #f9f7f1; /* Light newspaper-like background */
    display: flex;
    justify-content: center;
}

.Event-Container {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); /* Responsive grid */
    gap: 20px;
    width: 100%;
    max-width: 1200px;
}

.Event-Box {
    position: relative;
    background-size: cover;
    background-position: center;
    border: 2px solid #ff9800; /* Light orange border */
    padding: 20px;
    box-shadow: 5px 5px 0px rgba(0, 0, 0, 0.2);
    transition: transform 0.3s ease-in-out;
    min-height: 250px;
    display: flex;
    align-items: flex-end;
    justify-content: center;
}

.Event-Box:hover {
    transform: scale(1.03);
}

.box {
    background: rgba(255, 255, 255, 0.85); /* Semi-transparent white for a vintage feel */
    padding: 20px;
    border-radius: 5px;
    font-family: 'EB Garamond', serif; /* Classic newspaper-like font */
    text-align: center;
    width: 100%;
}

.box h3 {
    font-size: 22px;
    font-weight: 700;
    margin-bottom: 10px;
    color: #222;
    text-transform: uppercase;
}

.box p {
    font-size: 16px;
    color: #444;
    font-style: italic;
}

.box a {
    display: inline-block;
    margin-top: 10px;
    padding: 8px 12px;
    background: #ff9800;
    color: white;
    text-decoration: none;
    font-weight: bold;
    border-radius: 3px;
    box-shadow: 3px 3px 0px rgba(0, 0, 0, 0.2);
}

.box a:hover {
    background: #e68900;
    box-shadow: 4px 4px 0px rgba(0, 0, 0, 0.3);
}

/* Media Queries for Better Responsiveness */
@media (max-width: 768px) {
    .Event-Container {
        grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    }
}

@media (max-width: 480px) {
    .Event-Container {
        grid-template-columns: 1fr;
    }
}
@import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@400;700&display=swap');

.offering-section-container {
    background-color: #fffaf0; /* Light Orange Background */
}

.offering-section-header {
    font-size: 32px;
    font-weight: 700;
    margin-bottom: 30px;
    color: #d86f2d; /* Deep Orange */
    text-transform: uppercase;
    letter-spacing: 1px;
}

/* Flex Container for Even Spacing */
.offering-section {
    display: flex;
    flex-wrap: wrap; 
    overflow-x: auto;  

    gap: 12px;
}
.auto-slide{
    display: flex;
    flex-wrap: nowrap; 
    gap:15px;
}
/* Default Small Box */
.offering-section .box {
    position: relative;
    width: 120px;
    height: 300px;
    background-size: cover;  /* Ensures the image covers the entire box */
    background-position: center; /* Centers the image */
    background-repeat: no-repeat; /* Prevents repetition */
    overflow: hidden;
    border-radius: 8px;
    cursor: pointer;
    transition: all 0.2s ease-in-out;
    color: black;
    box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2);
}

/* Soft Orange Glow on Hover */
.offering-section .box:hover {
    width: 320px;
    box-shadow: 0 8px 20px rgba(216, 111, 45, 0.5);
    background-size: cover;  /* Ensures the image covers the entire box */
    background-position: center; /* Centers the image */
    background-repeat: no-repeat; /* Prevents repetition */
}

/* Overlay for Readability */
.offering-section .box::before {
    content: "";
    position: absolute;
    width: 100%;
    height: 100%;
    transition: opacity 0.2s ease-in-out;
}

.offering-section .box:hover::before {
    opacity: 0;
}

/* Vertical Headings by Default */
.offering-section .box h3 {
    writing-mode: vertical-rl;
    transform: rotate(180deg);
    font-size: 18px;
    font-weight: 700;
    color: #ffffff;
    background-color: rgba(0, 0, 0, 0.3);
    text-transform: uppercase;
    transition: transform 0.2s ease-in-out, font-size 0.2s;
}

/* Change to Horizontal on Hover */
.offering-section .box:hover h3 {
    writing-mode: horizontal-tb;
    transform: rotate(0);
    font-size: 22px;
    color: #d86f2d; /* Orange */
    background-color: rgba(0, 0, 0, 0.5);
    text-shadow: -1px -1px 0 #000,  
                  1px -1px 0 #000,  
                 -1px  1px 0 #000,  
                  1px  1px 0 #000; /* Creates a sharp black outline */
}

.offering-section .box:hover img{
    object-fit: cover;
}
/* Hidden Description (Appears on Hover) */
.offering-section .box p {
    font-size: 14px;
    color: #fff;
    background-color: rgba(0, 0, 0, 0.5);
    opacity: 0;
    transform: translateY(10px);
    transition: opacity 0.2s ease-in-out, transform 0.2s ease-in-out;
}

/* Show Description on Hover */
.offering-section .box:hover p {
    opacity: 1;
    transform: translateY(0);
}


/* Mobile responsive */
@media (max-width: 768px) {
    .search-box {
        width: 100%;
        left: 0;
        right: 0;
    }
    .title-hero{
        width: 100%;
    }
}
/* Responsive Design */
@media (max-width: 768px) {
    .offering-section {
        flex-direction: column;
        align-items: center;
    }

    .offering-section .box {
        width: 100%;
        height: 100px;
    }

    .offering-section .box:hover {
        height: 250px;
    }
}

.swiper-slide a{
    text-decoration: none;
    color: white;
}
.popup-message {
    position: fixed;
    bottom: 80px;
    right: 20px;
    background: #F4A261;
    color: white;
    padding: 10px 15px;
    border-radius: 5px;
    box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);
    font-size: 14px;
    display: none;
    z-index: 1000;
    animation: fadeIn 0.5s ease-in-out;
}

@keyframes fadeIn {
    from { opacity: 0; transform: translateY(10px); }
    to { opacity: 1; transform: translateY(0); }
}

/* New Offering Section Styles */
.offering-section-new {
    padding: 40px 5%;
    background-color: #f9f7f1;
}

.offering-grid {
    display: grid;
    grid-template-columns: repeat(3, minmax(300px, 1fr));
    gap: 30px;
    width: 100%;
    max-width: 1200px;
    margin: 0 auto;
}

.offering-card {
    position: relative;
    height: 200px;
    border-radius: 15px;
    overflow: hidden;
    box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
    transition: all 0.3s ease;
    cursor: pointer;
}

.offering-card:hover {
    transform: translateY(-5px);
    box-shadow: 0 8px 25px rgba(0, 0, 0, 0.2);
}

.offering-image {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.3s ease;
}

.offering-card:hover .offering-image {
    transform: scale(1.1);
}

.offering-content {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    padding: 20px;
    background: linear-gradient(to top, rgba(0, 0, 0, 0.8), transparent);
    color: white;
    transform: translateY(70px);
    transition: transform 0.3s ease;
}

.offering-card:hover .offering-content {
    transform: translateY(0);
}

.offering-title {
    font-size: 18px;
    font-weight: 600;
    margin-bottom: 10px;
    color: #F4A261;
}

.offering-description {
    font-size: 14px;
    opacity: 0;
    transition: opacity 0.3s ease;
    line-height: 1.4;
}

.offering-card:hover .offering-description {
    opacity: 1;
}

@media (max-width: 768px) {
    .offering-grid {
        grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    }
    
    .offering-card {
        height: 180px;
    }
}

