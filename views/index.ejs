/* New Offering Section Styles */
.offering-section-new {
    padding: 40px 5%;
    background-color: #f9f7f1;
}

.offering-grid {
    display: grid;
    grid-template-columns: repeat(3, minmax(300px, 1fr));
    gap: 30px;
    width: 100%;
    max-width: 1200px;
    margin: 0 auto;
}

.offering-card {
    position: relative;
    height: 200px;
    border-radius: 15px;
    overflow: hidden;
    box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
    transition: all 0.3s ease;
    cursor: pointer;
}

.offering-card:hover {
    transform: translateY(-5px);
    box-shadow: 0 8px 25px rgba(0, 0, 0, 0.2);
}

.offering-image {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.3s ease;
}

.offering-card:hover .offering-image {
    transform: scale(1.1);
}

.offering-content {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    padding: 20px;
    background: linear-gradient(to top, rgba(0, 0, 0, 0.8), transparent);
    color: white;
    transform: translateY(70px);
    transition: transform 0.3s ease;
}

.offering-card:hover .offering-content {
    transform: translateY(0);
}

.offering-title {
    font-size: 18px;
    font-weight: 600;
    margin-bottom: 10px;
    color: #F4A261;
}

.offering-description {
    font-size: 14px;
    opacity: 0;
    transition: opacity 0.3s ease;
    line-height: 1.4;
}

.offering-card:hover .offering-description {
    opacity: 1;
}

@media (max-width: 768px) {
    .offering-grid {
        grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    }
    
    .offering-card {
        height: 180px;
    }
}

